{
  "name": "zleslie-pf",
  "version": "0.1.2",
  "author": "zleslie",
  "summary": "Manage PF with Puppet",
  "license": "Apache-2.0",
  "source": "git://github.com/xaque208/puppet-pf.git",
  "project_page": "https://github.com/xaque208/puppet-pf",
  "issues_url": "https://github.com/xaque208/puppet-pf/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "10.1",
        "10.0",
        "9.3",
        "8.4"
      ]
    },
    {
      "operatingsystem": "OpenBSD",
      "operatingsystemrelease": [
        "5.7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.2.0 < 3.8.0"
    },
    {
      "name": "puppet",
      "version_requirement": "3.x"
    }
  ],
  "description": "Manage the OpenBSD Packet Filtter with Puppet",
  "dependencies": [
    {"name":"puppetlabs/concat","version_requirement":">=1.2.1"},
    { "name": "puppetlabs/stdlib", "version_requirement": ">= 4.0.0" },
    { "name": "dalen/puppetdbquery", "version_requirement": ">= 1.3.0" }
  ]
}
